	"license": "MIT",
	"devDependencies": {
		"@types/inquirer": "^9.0.3",
		"tsup": "^6.7.0"
	},
	"dependencies": {
		"axios": "^1.4.0",
		"inquirer": "^9.2.10",
		"node": "^20.5.0"
	}
}
