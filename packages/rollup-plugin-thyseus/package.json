{
	"name": "@thyseus/rollup-plugin-thyseus",
	"version": "0.16.0-beta.0",
	"description": "Rollup plugin for the Thyseus transformer",
	"author": "Jaime Gensler",
	"license": "MIT",
	"homepage": "https://thyseus.dev",
	"repository": {
		"type": "git",
		"url": "https://github.com/JaimeGensler/thyseus"
	},
	"keywords": [
		"thyseus",
		"typescript",
		"transformer",
		"transpiler",
		"compiler",
		"rollup"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"module": "./dist/index.js",
	"main": "./dist/index.umd.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		"./src/*": "./src/*",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		}
	},
	"scripts": {
		"dev": "pnpm test",
		"test": "vitest",
		"build": "vite build && tsup src/index.ts --dts-only"
	},
	"devDependencies": {
		"rollup": "^3.26.0",
		"thyseus": "0.14.0",
		"tsup": "^6.7.0",
		"vite": "^4.3.9",
		"vitest": "^0.29.8"
	},
	"dependencies": {
		"@rollup/pluginutils": "^5.0.2",
		"@thyseus/typescript-transformer": "workspace:*",
		"typescript": "^5.1.6"
	},
	"peerDependencies": {
		"thyseus": "^0.16.0-beta.0"
	}
}
